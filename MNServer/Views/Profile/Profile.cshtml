@model MNServer.Models.Tbl_Customer_Master
@{
    Layout = null;
    var viewTemp = @TempData["Viewstr"];

}

<!DOCTYPE html>

<html>

<head>

    <meta name="viewport" content="width=device-width" />
    <script src="~/Scripts/jquery-1.8.3.min.js"></script>
    <link href="~/Scripts/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <link href="~/Scripts/Test.css" rel="stylesheet" />
</head>

<body>

    <script src="~/Scripts/jquery-1.7.1.min.js"></script>
    <link href="~/Script/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Script/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <link href="~/Script/Test.css" rel="stylesheet" />

    <div class="text-center form-group Heading" style="padding:10px;">User Profile</div>
    <div class="panel panel-body panel-heading col-md-8" style=" border: 2px groove #ddd !important; border-radius: 7px; margin-left:17%;">

        @using (Html.BeginForm(FormMethod.Post))
        {

            @Html.ValidationSummary(true)

            <fieldset>
                <div class="ValidateLable">
                    <p>@ViewBag.MyString</p>
                </div>

                @if (viewTemp.Equals("O"))
                {
                    <div class="FontStyle">
                        <div class="form-row">
                            <div id="DivDummy" class="col-md-1 "></div>
                            <div class="form-group col-md-4">

                                First Name *
                                @Html.TextBoxFor(m => m.FirstName, new { placeholder = "Enter First Name", @class = "form-control" })
                                @Html.ValidationMessageFor(m => m.FirstName, "", new { @class = "text-danger" })
                            </div>
                            <div id="DivDummy" class="col-md-2 "></div>
                            <div class="form-group col-md-4">
                                Last Name *
                                @Html.TextBoxFor(m => m.LastName, new { placeholder = "Enter Last Name", @class = "form-control" })
                                @Html.ValidationMessageFor(m => m.LastName, "", new { @class = "text-danger" })

                            </div>


                        </div>

                        <div class="form-row">
                            <div id="DivDummy" class="col-md-1 "></div>
                            <div class="form-group col-md-4">
                                Mobile Number
                                @Html.TextBoxFor(m => m.Mob_no, new { placeholder = "Enter Mobile Number", @class = "form-control", maxlength = "10" })
                                @Html.ValidationMessageFor(m => m.Mob_no, "", new { @class = "text-danger" })

                            </div>
                            <div id="DivDummy" class="col-md-2 "></div>
                            <div class="form-group col-md-4">
                                Email ID
                                @Html.TextBoxFor(m => m.EmailID, new { placeholder = "Enter Email Address", @class = "form-control" })
                                @Html.ValidationMessageFor(m => m.EmailID, "", new { @class = "text-danger" })
                            </div>

                        </div>

                        <div class="form-row">
                            <div id="DivDummy" class="col-md-1 "></div>
                            <div class="form-group col-md-4">
                                Id Proof Type
                                @Html.DropDownList("IdProof", new List<SelectListItem>
                                {
                                   new SelectListItem{ Text="----Select ID Proof Type----" },
                                   new SelectListItem{ Text="Election Voter ID" },
                                   new SelectListItem{ Text="Pan" },
                                   new SelectListItem{ Text="Aadhar" },
                                },
                          new { @class = "form-control" }
                                                )

                            </div>
                            <div id="DivDummy" class="col-md-2 "></div>
                            <div class="form-group col-md-4">
                                Id Proof Number
                                @Html.TextBoxFor(m => m.OtherGovID, new { placeholder = "Enter Id Proof Number", @class = "form-control" })
                                @Html.ValidationMessageFor(m => m.OtherGovID, "", new { @class = "text-danger" })
                            </div>

                        </div>
                        @*<div class="panel panel-body panel-heading col-md-10" style=" border: 1px groove #ddd !important; border-radius: 7px; margin-left:8%; text-align:center; font-size:x-large ; background-color:#ddd">Address</div>*@
                        @*<div class="form-row">
                                        <div id="DivDummy" class="col-md-2 "></div>
                                        <div class="form-group col-md-6">
                                            <b> Are You Indian</b><br />
                                            <span>Yes</span> @Html.RadioButton("Indian", "Indian", true)
                                            <span>No</span> @Html.RadioButton("Indian", "Indian", false)
                                        </div>

                            </div>*@
                        <div class="form-row">
                            <div id="DivDummy" class="col-md-1 "></div>
                            <div class="form-group col-md-4">
                                House Number
                                @Html.TextBoxFor(m => m.Address, new { placeholder = "Enter House Number", @class = "form-control" })
                                @Html.ValidationMessageFor(m => m.Address, "", new { @class = "text-danger" })

                            </div>
                            <div id="DivDummy" class="col-md-2 "></div>
                            @*<div class="form-group col-md-4">
                                    Pin Code
                                    @Html.TextBoxFor(m => m.PinCode, new { placeholder = "Enter Pin Code", @class = "form-control" })
                                    @Html.ValidationMessageFor(m => m.PinCode, "", new { @class = "text-danger" })

                                </div>*@

                        </div>
                        <div class="form-row">
                            <div id="DivDummy" class="col-md-1 "></div>
                            @*<div class="form-group col-md-4">
                                    City Name
                                    @Html.TextBoxFor(m => m.City, new { placeholder = "Enter City", @class = "form-control" })
                                    @Html.ValidationMessageFor(m => m.City, "", new { @class = "text-danger" })

                                </div>*@
                            <div id="DivDummy" class="col-md-2 "></div>

                        </div>
                    </div>

                }

                else
                {
                    <div class="form-horizontal" style="margin-bottom:0px;">
                        <div class="panel panel-body panel-heading col-md-8" style=" border: 2px groove #ddd !important; border-radius: 7px; margin-left:17%;">

                            @using (Html.BeginForm(FormMethod.Post))
                            {

                                @Html.ValidationSummary(true)

                                <div class="ValidateLable">
                                    <p>@ViewBag.MyString</p>
                                </div>
                                <div class="FontStyle">
                                    <div class="form-row">
                                        <div id="DivDummy" class="col-md-1 "></div>
                                        <div class="form-group col-md-4">

                                            Organisation Name
                                            @Html.TextBoxFor(m => m.OrganisationName, new { placeholder = "Enter  Organisation Name ", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.OrganisationName, "", new { @class = "text-danger" })
                                        </div>
                                        <div id="DivDummy" class="col-md-2 "></div>


                                    </div>

                                    @*<div class="form-row">
                                            <div id="DivDummy" class="col-md-1 "></div>
                                            <div class="form-group col-md-4">
                                                Organisation Certificate
                                                @Html.TextBoxFor(m => m.OrganisationCertificate, new { placeholder = "Enter Organisation Certificate ", @class = "form-control" })
                                                @Html.ValidationMessageFor(m => m.OrganisationCertificate, "", new { @class = "text-danger" })

                                            </div>
                                            <div id="DivDummy" class="col-md-2 "></div>
                                            <div class="form-group col-md-4">
                                                Organisation Certificate Number
                                                @Html.TextBoxFor(m => m.OrganisationCertificateNUmber, new { placeholder = "Enter Certificate Number", @class = "form-control" })
                                                @Html.ValidationMessageFor(m => m.OrganisationCertificateNUmber, "", new { @class = "text-danger" })
                                            </div>

                                        </div>*@

                                    <div class="form-row">
                                        <div id="DivDummy" class="col-md-1 "></div>
                                        <div class="form-group col-md-4">
                                            Landline Number
                                            @*@Html.DropDownListFor(m => m.IdProof, new SelectList(Enum.GetValues(typeof(IdProof))), " --------Select IdProof--------", new { @class = "form-control" })*@
                                            @Html.TextBoxFor(m => m.LandlineNo, new { placeholder = "Enter Landline Number ", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.LandlineNo, "", new { @class = "text-danger" })

                                        </div>
                                        <div id="DivDummy" class="col-md-2 "></div>
                                        <div class="form-group col-md-4">
                                            Email Address
                                            @Html.TextBoxFor(m => m.EmailID, new { placeholder = "Enter Email Address ", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.EmailID, "", new { @class = "text-danger" })
                                        </div>

                                    </div>

                                    <div class="form-row">
                                        <div id="DivDummy" class="col-md-1 "></div>
                                        <div class="form-group col-md-4">
                                            Nationality

                                            @Html.TextBoxFor(m => m.Nationality, new { placeholder = "Enter Nationality", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.Nationality, "", new { @class = "text-danger" })

                                        </div>
                                        <div id="DivDummy" class="col-md-2 "></div>
                                        <div class="form-group col-md-4">
                                            City
                                            @Html.TextBoxFor(m => m.City, new { placeholder = "Enter  City ", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.City, "", new { @class = "text-danger" })

                                        </div>

                                    </div>
                                    <div class="form-row">
                                        <div id="DivDummy" class="col-md-1 "></div>
                                        <div class="form-group col-md-4">
                                            Pin Code
                                            @*@Html.DropDownListFor(m => m.State, new SelectList(Enum.GetValues(typeof(State))), "   --------Select State--------", new { @class = "form-control" })*@
                                            @Html.TextBoxFor(m => m.Pincode, new { placeholder = "Enter Pin Code", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.Pincode, "", new { @class = "text-danger" })

                                        </div>
                                        <div id="DivDummy" class="col-md-2 "></div>
                                        <div class="form-group col-md-4">

                                            State
                                            @*@Html.DropDownListFor(m => m.State, new SelectList(Enum.GetValues(typeof(State))), "   --------Select State--------", new { @class = "form-control" })*@
                                            @Html.TextBoxFor(m => m.State, new { placeholder = "Enter State", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.State, "", new { @class = "text-danger" })
                                        </div>
                                    </div>

                                    <div class="form-row">
                                        <div id="DivDummy" class="col-md-1 "></div>
                                        <div class="form-group col-md-4">
                                            Authorized Person Name
                                            @Html.TextBoxFor(m => m.AutherizedPerson, new { placeholder = "Enter Authorized Person Name", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.AutherizedPerson, "", new { @class = "text-danger" })

                                        </div>
                                        <div id="DivDummy" class="col-md-2 "></div>
                                        <div class="form-group col-md-4">

                                            Authorized Person Designation
                                            @*@Html.DropDownListFor(m => m.State, new SelectList(Enum.GetValues(typeof(State))), "   --------Select State--------", new { @class = "form-control" })*@
                                            @Html.TextBoxFor(m => m.AutherizPrsDesig, new { placeholder = "Enter Authorized Person Designation", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.AutherizPrsDesig, "", new { @class = "text-danger" })
                                        </div>
                                    </div>
                                    <div class="form-row">
                                        <div id="DivDummy" class="col-md-1 "></div>
                                        <div class="form-group col-md-4">
                                            Authorized Person Email Id
                                            @Html.TextBoxFor(m => m.AutherizPrsEmail_Id, new { placeholder = "Enter Authorized Person Email Id", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.AutherizPrsEmail_Id, "", new { @class = "text-danger" })

                                        </div>
                                        <div id="DivDummy" class="col-md-2 "></div>
                                        <div class="form-group col-md-4">

                                            Authorized Person Mobile Number
                                            @*@Html.DropDownListFor(m => m.State, new SelectList(Enum.GetValues(typeof(State))), "   --------Select State--------", new { @class = "form-control" })*@
                                            @Html.TextBoxFor(m => m.AutherizPrsMobile_No, new { placeholder = "Enter Authorized Person Mob Number", @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.AutherizPrsMobile_No, "", new { @class = "text-danger" })
                                        </div>
                                    </div>
                                </div>

                            }
                        </div>
                    </div>
                }
            </fieldset>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10" style="margin-left: 340px; padding: 0px;">
                    <input type="button" class="btn-primary" value="Indivisual" checked="checked" onclick="location.href='@Url.Action("GetprofileView","Profile")'">
                    <input type="submit" value="Reset" class="btn btn-primary" />
                </div>
            </div>

        }

        <div>

            <h5 style="color:red">@TempData["Responce"]</h5>

        </div>
    </div>
    }

    </div>


</body>

</html>
